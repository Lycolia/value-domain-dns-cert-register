{
  "name": "@lycolia/value-domain-dns-cert-register",
  "version": "0.0.1-6",
  "description": "Automatically add dns certificates for value-domain using on certbot manual mode",
  "keywords": [
    "automation",
    "certbot",
    "value-domain",
    "dns"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Lycolia/value-domain-dns-cert-register"
  },
  "bin": {
    "vddcr": "./src/index.js"
  },
  "main": "./src/index.js",
  "author": "Lycolia",
  "license": "MIT",
  "scripts": {
    "start": "npm run build-ts && node dist/index.js",
    "dev": "npm run build-ts && node --inspect-brk dist/index.js",
    "type-check": "tsc --noEmit && eslint .",
    "build": "npm run type-check && npm run build-ts",
    "build-ts": "rm -Rf dist/ && swc src/ --config-file .swcrc_build -d dist/",
    "test": "jest --watch",
    "coverage": "jest --coverage"
  },
  "dependencies": {
    "axios": "^0.21.0",
    "chalk": "^4.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.2.0",
    "@lycolia/eslint-config": "^0.9.1",
    "@swc/cli": "^0.1.65",
    "@swc/core": "^1.3.105",
    "@swc/jest": "^0.2.31",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.11.5",
    "eslint": "^8.57.0",
    "eslint-plugin-jest": "^28.5.0",
    "jest": "^29.7.0",
    "jest-watch-typeahead": "^2.2.2",
    "prettier": "^3.2.5",
    "typescript": "^5.3.3",
    "typescript-eslint": "^7.8.0"
  }
}
